<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="kitchen_dining">
    <static>true</static>
    <link name="link">
      
      <!-- Ensure the model is grounded -->
      <pose>0 0 0 0 0 0</pose>  <!-- Adjust Z if needed (e.g., -0.1) -->

      <!-- Visual geometry -->
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://kitchen_dining/meshes/kitchen_dining.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <!-- Collision geometry for physics -->
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://kitchen_dining/meshes/kitchen_dining.dae</uri>
          </mesh>
        </geometry>
      </collision>

    </link>
  </model>
</sdf>
